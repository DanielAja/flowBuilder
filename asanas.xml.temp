      <tag>Strength</tag>
      <tag>Heart Opener</tag> <!-- Slight chest opening -->
    </tags>
    <transitions>
      <transition>Warrior 2</transition>
      <transition>Extended Side Angle</transition>
      <transition>Triangle Pose</transition>
      <transition>Half Moon Pose</transition>
    </transitions>
